{
  "name": "contentful-static-react",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "contentful": "^5.1.3",
    "marksy": "^6.0.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-overlay-menu": "^1.0.0",
    "react-responsive-carousel": "^3.1.40",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "react-slick": "^0.23.1",
    "react-snap": "^1.11.5",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/scss -o ./src",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/scss -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "postbuild": "react-snap",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
